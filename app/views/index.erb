<!DOCTYPE html>
<html>
    <head>
        <title>Thermostat</title>
        <link rel='stylesheet' href='css/interface.css'>
        <link href="https://fonts.googleapis.com/css?family=Josefin+Sans" rel="stylesheet">
    </head>
    <body id="body">

      <p>
        <h1 id="current-temp"></h1>
        <button id="up">Up</button>

        <button id="down">Down</button>
      </p>

      <p>
        Power saving mode <span id="power-saving-status">on</span> <br>
        <button id="power-savings-on">Power Savings On</button>
        <button id="power-savings-off">Power Savings Off</button>
      </p>

      <section>
        Outside Temperature in <span id='citytemp'>London</span> is: <span id="outside-temperature"></span>
        <input id='cityname'></input><button id='citysub'>Submit</input>
      </section>


      <button id="reset">Reset</button>

      <script src="https://code.jquery.com/jquery-3.1.1.js"
              integrity="sha256-16cdPddA6VdVInumRGo6IbivbERE8p7CQR3HzTBuELA="
              crossorigin="anonymous">
      </script>

      <script src='js/Thermostat.js'></script>
      <script src='js/interface.js'></script>
    </body>
</html>
